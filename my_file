FROM ubuntu:latest

ENV http_proxy http://cache-adm.univ-artois.fr:8080/
ENV https_proxy https://cache-adm.univ-artois.fr:8080/

RUN apt-get -y update && apt-get install -y wget zip \
    && mkdir instancescompet2016Appli \
    && wget https://baldur.iti.kit.edu/sat-competition-2016/downloads/app16.zip\
    && unzip Main.zip -d instancescompet2016Appli \
&& rm -rf Main.zip	
